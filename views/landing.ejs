<% include partials/header %>

    <div>
        <h1>Landing Page</h1>
    </div>
    <div>
        <table style="width:100% ">
            <tr>
                <th>Name</th>
                <th>Price</th>
                <th>MoreInfo</th>
            </tr>
            <% books.forEach(function(book){%>
                <tr>
                    <td><%= book.name %></td>
                    <td><%= book.price %></td>
                    <td><a href="<%= book.url %>">Get Info</a></td>
                </tr>
            <% }) %>
        </table>
    </div>
    <div>
            I want to <a href="/login">purchase something! </a>
    </div>


    <% include partials/footer %>